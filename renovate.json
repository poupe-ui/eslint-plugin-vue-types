{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependencies"
  ],
  "postUpdateOptions": [
    "pnpmDedupe"
  ],
  "packageRules": [
    {
      "groupName": "eslint",
      "matchPackageNames": [
        "/^eslint$/",
        "/^@eslint//",
        "/^@types/eslint/"
      ]
    }
  ]
}
